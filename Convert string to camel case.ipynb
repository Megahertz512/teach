   "source": [
    "import re\n",
    "def to_camel_case(text):\n",
    "    a = re.split('_|-', text)\n",
    "    b = a[0]\n",
    "    for i in range(1,len(a)):\n",
    "        a[i] = a[i][0].capitalize()+a[i][1:]\n",
    "        b += a[i]\n",
    "    return b"
   ]
  },
